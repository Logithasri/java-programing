public class Main {
    public static void main(String[] args) {
        int[] arr = {1, 2, 3, 4};
        int a = 1;
        int b = 4;

        int[] sub = new int[b - a];
        for (int i = a; i < b; i++) {
            sub[i - a] = arr[i];  
        }

        for (int num : sub) {
            System.out.print(num + " ");
        }
        System.out.println();
    }
}
2 3 4 

