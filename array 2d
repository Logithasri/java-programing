class Main {
    public static void main(String[] args) {
        int x=2;
        int y=2;
        int[][] arr=new int[x][y];
        int num=1;
        for (int i = 0; i < x; i++) {
            for (int j = 0; j < y; j++) {
                arr[i][j]=num;
                System.out.print(arr[i][j]+" ");
                num++;
            }
            System.out.println();
     }
  }
}

