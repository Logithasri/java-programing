import java.util.Scanner;

class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        
        String x ="spiderman";

        
        String sub=x.substring(0,6);
        String sub1=x.substring(6);
        System.out.println(sub+" "+sub1);
    }
}

